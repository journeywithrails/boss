	<% @content_for_lnav = capture do %>
	<% end %>
    
	<% @content_for_sidebar = capture do %>
	<% end %>

	<% @content_for_main = capture do %>
		<h1><%= _("Listing deliveries") %></h1>
		
		<table>
		  <tr>
		    <th><%= _("Item") %></th>
		    <th><%= _("Status") %></th>
		    <th><%= _("Date") %></th>
		  </tr>
		
		<% for delivery in @deliveries %>
		  <tr>
		    <td><%= delivery.deliverable_type.humanize %></td>
		    <td><%= delivery.status.titleize %></td>
		    <td><%= delivery.created_at %></td>
		
		  </tr>
		<% end %>
		</table>
		
		<br />
	<% end %>
	<%= render_inner_layout %>
